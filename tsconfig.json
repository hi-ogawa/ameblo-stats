{
  "include": ["app"],
  "compilerOptions": {
    "rootDir": "app",
    "outDir": "build/tsc",
    "module": "commonjs",
    "target": "es2021",
    "lib": ["es2021", "dom"],
    "types": [],
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "allowJs": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "baseUrl": ".",
    "noEmit": true
  }
}
